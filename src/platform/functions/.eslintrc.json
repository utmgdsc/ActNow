{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "arrow-body-style": 0,
    "prefer-template": 0,
    "operator-linebreak": 0,
    "no-unused-vars": 1,
    "implicit-arrow-linebreak": 0,
    "function-paren-newline": 0,
    "consistent-return": 0
  }
}
